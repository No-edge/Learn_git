<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
urgent chan Callwaiter;
chan dishescome;
chan Waitercome;
broadcast chan SendOrder;
const int min1 = 60;
const int min5 = 300;
const int min10 = 600;
const int min15 = 900;
clock total;</declaration>
	<template>
		<name x="5" y="5">Customer</name>
		<declaration>// Place local declarations here.
clock x;</declaration>
		<location id="id0" x="0" y="0">
			<name x="-42" y="-34">Enter_restaurant</name>
		</location>
		<location id="id1" x="153" y="0">
			<name x="119" y="-34">Wait_server</name>
			<label kind="invariant" x="136" y="17">x&lt;=min5</label>
		</location>
		<location id="id2" x="433" y="0">
			<name x="416" y="-34">Wait_dishes</name>
			<label kind="invariant" x="390" y="17">x&lt;=min15</label>
		</location>
		<location id="id3" x="603" y="0">
			<name x="593" y="-34">Serve_over</name>
			<committed/>
		</location>
		<location id="id4" x="331" y="195">
			<name x="297" y="161">Get_angry</name>
			<label kind="invariant" x="306" y="204">x&lt;=min5</label>
		</location>
		<location id="id5" x="331" y="331">
			<name x="288" y="297">Leave_restaurant</name>
			<committed/>
		</location>
		<location id="id6" x="297" y="0">
			<name x="278" y="-34">Serve</name>
			<urgent/>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id6"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="323" y="-17">SendOrder?</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="476" y="68">dishescome?</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="272" y="51">Waitercome?</label>
			<label kind="assignment" x="297" y="68">x=0</label>
			<nail x="306" y="59"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id4"/>
			<label kind="guard" x="204" y="59">x&gt;min1</label>
			<label kind="assignment" x="204" y="76">x=0</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="guard" x="323" y="246">x&gt;min1</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id4"/>
			<label kind="guard" x="340" y="85">x&gt;=min10</label>
			<label kind="assignment" x="348" y="102">x=0</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="467" y="-17">dishescome?</label>
			<nail x="442" y="0"/>
			<nail x="450" y="0"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="187" y="-17">Waitercome?</label>
			<label kind="assignment" x="221" y="0">x=0</label>
			<nail x="162" y="0"/>
			<nail x="289" y="0"/>
			<nail x="280" y="0"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="42" y="-17">Callwaiter!</label>
			<label kind="assignment" x="42" y="0">x=0</label>
		</transition>
	</template>
	<template>
		<name>Server</name>
		<declaration>clock x,wait;
const int lower=300;
const int upper=301;</declaration>
		<location id="id7" x="-2252" y="-1538">
			<name x="-2295" y="-1572">wait_customer</name>
			<urgent/>
		</location>
		<location id="id8" x="-1836" y="-1538">
			<name x="-1878" y="-1572">Serve_customer</name>
			<urgent/>
		</location>
		<location id="id9" x="-1674" y="-1538">
			<name x="-1708" y="-1572">Serve_over</name>
		</location>
		<location id="id10" x="-2057" y="-1538">
			<name x="-2099" y="-1572">Waiter_come</name>
		</location>
		<location id="id11" x="-1955" y="-1445">
			<name x="-1965" y="-1479">test</name>
			<committed/>
		</location>
		<init ref="id7"/>
		<transition>
			<source ref="id11"/>
			<target ref="id8"/>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id11"/>
			<label kind="guard" x="-2057" y="-1496">wait &gt;= upper</label>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id8"/>
			<label kind="guard" x="-2006" y="-1572">wait &gt;= lower</label>
			<label kind="synchronisation" x="-1997" y="-1555">Waitercome!</label>
			<label kind="assignment" x="-1989" y="-1538">wait=0</label>
		</transition>
		<transition>
			<source ref="id8"/>
			<target ref="id9"/>
			<label kind="synchronisation" x="-1802" y="-1555">SendOrder!</label>
			<label kind="assignment" x="-1793" y="-1538">wait=0</label>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id10"/>
			<label kind="synchronisation" x="-2184" y="-1555">Callwaiter?</label>
			<label kind="assignment" x="-2176" y="-1538">wait=0</label>
			<nail x="-2082" y="-1538"/>
			<nail x="-2091" y="-1538"/>
		</transition>
	</template>
	<template>
		<name>Chef</name>
		<declaration>clock x,cook;
const int lower=900;
const int upper=901;</declaration>
		<location id="id12" x="-501" y="-187">
			<name x="-535" y="-221">WaitOrder</name>
		</location>
		<location id="id13" x="-331" y="-187">
			<name x="-365" y="-221">Make_dishes</name>
		</location>
		<location id="id14" x="-144" y="-187">
			<name x="-188" y="-221">Finish_dishes</name>
			<committed/>
		</location>
		<location id="id15" x="-17" y="-187">
			<name x="-27" y="-221">Over</name>
			<urgent/>
		</location>
		<location id="id16" x="-238" y="-102">
			<name x="-248" y="-136">test</name>
			<committed/>
		</location>
		<init ref="id12"/>
		<transition>
			<source ref="id16"/>
			<target ref="id14"/>
		</transition>
		<transition>
			<source ref="id13"/>
			<target ref="id16"/>
			<label kind="guard" x="-331" y="-153">cook &gt;= upper</label>
		</transition>
		<transition>
			<source ref="id14"/>
			<target ref="id15"/>
			<label kind="synchronisation" x="-127" y="-204">dishescome!</label>
		</transition>
		<transition>
			<source ref="id13"/>
			<target ref="id14"/>
			<label kind="guard" x="-297" y="-204">cook &gt;= lower</label>
			<label kind="assignment" x="-263" y="-187">cook = 0</label>
		</transition>
		<transition>
			<source ref="id12"/>
			<target ref="id13"/>
			<label kind="synchronisation" x="-450" y="-204">SendOrder?</label>
			<label kind="assignment" x="-442" y="-187">cook = 0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Customer();
Process1 = Server();
Process2 = Chef();
// List one or more processes to be composed into a system.
system Process, Process1, Process2;
    </system>
	<queries>
		<query>
			<formula>E[] (not Process.Get_angry) and Process2.test</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E[] (not Process.Get_angry) and Process1.test</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E[] not Process.Get_angry</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
